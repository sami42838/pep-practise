

import java.util.*;
import java.lang.*;
import java.io.*;

class GFG {
	public static void main (String[] args) {
	    Scanner k = new Scanner(System.in);
	    int n=k.nextInt();
	    
	    int[] arr = new int[n];
	    
	    for(int i=0;i<n;i++){
	        arr[i] = k.nextInt();
	    }
		int smallest = arr[0];
		
		for(int i=0;i<n;i++){
		    if(arr[i] < smallest){
		        smallest = arr[i];
		    }
		}
		System.out.println(smallest);
		
	}
}
