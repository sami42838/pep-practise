import java.util.Scanner;
public class Main{
    public static void main(String[]args);
    Scanner k = new Scanner(System.in);

    int n = k.nextInt();

    int [] arr = new int[n];

    for(int i = 0;i<n; i++){
        arr[i] = k.nextInt();
    }

    int [] temp = new int[n];
    int j = 0;
    for(int i = n-1; i>=0; i--){
        temp[j] = arr[i];
        j++;
    }

    for(int i=0;i<n;i++){
        Sytem.out.print("Reversed array is: " + temp[i]);
    }
}
